<?xml version = "1.0" standalone="no"?>
<!DOCTYPE template SYSTEM "../dtd/file_template.dtd">
<!-- *modified "Tue Aug  1 11:48:37 2023" *by "Paul E. Black" -->

<template name="Python">
    <file_extension>py</file_extension>
    <comment>
        <open>'''</open>
        <close>'''</close>
        <inline>#</inline>
    </comment>
    <syntax>
      <!-- If language doesn't have a terminator, leave empty-->
      <statement_terminator></statement_terminator>
      <!-- Insert desired indentation for one level -->
      <indent>    </indent>
      <import_code>import {{import_file}}</import_code>
    </syntax>
    <variables prefix="">
    </variables>
    <imports>
        <import>sys</import>
        <import>math</import>
    </imports>
    <code>
'''
{{ comments }}
'''

'''
{{ license }}
'''
{{ stdlib_imports }}

{{ static_methods }}


def main():
INDENT
    {{input_content}}
    {{filtering_content}}
    {{sink_content}}
    {{exec_queries_content}}
DEDENT

if __name__ == '__main__':
        main()
    </code>
</template>

<!-- end of file_template.xml -->
