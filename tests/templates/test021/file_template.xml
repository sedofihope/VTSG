<!-- test inconsistent declaring local variables, that is,
     mismatch between {{Local_var}} and <variable /> statements -->
<template name="test020">
    <file_extension>py</file_extension>
    <comment>
        <open>'''</open>
        <close>'''</close>
        <inline>#</inline>
    </comment>
    <syntax>
      <!-- If language doesn't have a terminator, leave empty-->
      <statement_terminator></statement_terminator>
      <!-- Insert desired indentation level using spaces-->
      <indent>    </indent>
    </syntax>
    <namespace></namespace>
    <variables prefix="" import_code="import {{import_file}}">
        <variable type="int" code="" init="0"/>
        <variable type="string" code="" init="None"/>
	<variable type="bool" code="" init="False"/>
	<variable type="float" code="" init="0.0"/>
    </variables>
    <imports>
    </imports>
    <code>
'''
{{ comments }}
'''

'''
{{ license }}
'''
{{ stdlib_imports }}

{{ static_methods }}


def main():
    INDENT
    {{input_content}}
    {{filtering_content}}
    {{sink_content}}
    DEDENT

if __name__ == '__main__':
        main()
    </code>
</template>

<!-- end of file_template.xml -->
