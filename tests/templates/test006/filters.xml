    <filtering>

        <sample>
            <path>
                <dir>no_filtering</dir>
            </path>
            <comment>no filtering</comment>
            <flaws>
                <flaw flaw_type="default" safe="0" unsafe="0"/>
            </flaws>
            <imports></imports>
            <code>
        # No filtering (sanitization)
        {{out_var_name}} = {{in_var_name}}
            </code>
            <input_type>nofilter</input_type>
            <output_type>nofilter</output_type>
        </sample>

    </filtering>
